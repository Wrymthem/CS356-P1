public class Student {
    
    private int iD;
    private String name;
    
    public Student (int iD, String name) {
        this.setID(iD);
        this.setName(name);
    }
    
    public void setName(String name) {
        this.name = name;
    }
    
    public void setID(int iD) {
        this.iD = iD;
    }
    
    public String getName() {
        return this.name;
    }
    
    public int getID() {
        return this.iD;
    }
}